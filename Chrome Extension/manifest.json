{
  "manifest_version": 2,

  "name": "Specialist 2.0 v3 Extension",
  "description": "This extension will get Adelphi Implementation Category",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  
     
   "content_scripts":[
       {
         "matches": ["http://*/*", "https://*/*"],
         "js": ["contentScript.js"]
       }
     ]
}